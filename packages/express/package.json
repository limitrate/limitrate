{
  "name": "@limitrate/express",
  "version": "2.2.0",
  "description": "Express middleware for LimitRate rate limiting and cost control",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "echo 'Linting @limitrate/express'",
    "clean": "rm -rf dist *.tsbuildinfo"
  },
  "keywords": [
    "express",
    "middleware",
    "rate-limiting",
    "rate-limit",
    "cost-control",
    "limitrate"
  ],
  "author": "LimitRate",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/limitrate/limitrate"
  },
  "bugs": {
    "url": "https://github.com/limitrate/limitrate/issues"
  },
  "homepage": "https://github.com/limitrate/limitrate#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@limitrate/core": "workspace:*"
  },
  "peerDependencies": {
    "express": "^4.18.0 || ^5.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/supertest": "^6.0.3",
    "express": "^4.18.2",
    "supertest": "^7.1.4",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}
