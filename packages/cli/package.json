{
  "name": "@limitrate/cli",
  "version": "2.1.1",
  "description": "CLI dashboard for LimitRate rate limiting inspection",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "limitrate": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./storage": {
      "types": "./dist/storage.d.ts",
      "import": "./dist/storage.mjs",
      "require": "./dist/storage.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/cli.ts src/storage.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts src/cli.ts src/storage.ts --format cjs,esm --dts --watch",
    "test": "echo 'No tests for CLI package yet'",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "echo 'Linting @limitrate/cli'",
    "clean": "rm -rf dist *.tsbuildinfo"
  },
  "keywords": [
    "limitrate",
    "cli",
    "dashboard",
    "rate-limiting",
    "monitoring"
  ],
  "author": "LimitRate",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/limitrate/limitrate"
  },
  "bugs": {
    "url": "https://github.com/limitrate/limitrate/issues"
  },
  "homepage": "https://github.com/limitrate/limitrate#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@limitrate/core": "workspace:*",
    "better-sqlite3": "^9.4.0",
    "cli-table3": "^0.6.3",
    "commander": "^11.1.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.8",
    "@types/node": "^20.11.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}
